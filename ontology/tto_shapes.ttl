@prefix core: <https://w3id.org/pmd/co/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix tto: <https://w3id.org/pmd/tto/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

[] a sh:NodeShape ;
    sh:property [ sh:minCount 1 ;
            sh:node [ a sh:NodeShape ;
                    sh:property [ sh:datatype xsd:float ;
                            sh:minCount 1 ;
                            sh:minInclusive "0"^^xsd:float ;
                            sh:path rdf:value ],
                        [ sh:hasValue <mm> ;
                            sh:minCount 1 ;
                            sh:path core:unit ] ] ;
            sh:path tto:OriginalGaugeLength ],
        [ sh:datatype xsd:string ;
            sh:minCount 1 ;
            sh:path core:PrimaryData ],
        [ sh:datatype xsd:string ;
            sh:minCount 1 ;
            sh:path core:SecondaryData ],
        [ sh:minCount 1 ;
            sh:node [ a sh:NodeShape ;
                    sh:property [ sh:hasValue <MPa> ;
                            sh:minCount 1 ;
                            sh:path core:unit ],
                        [ sh:datatype xsd:float ;
                            sh:minCount 1 ;
                            sh:minInclusive "0"^^xsd:float ;
                            sh:path rdf:value ] ] ;
            sh:path tto:UltimateTensileStrength ],
        [ sh:minCount 1 ;
            sh:node [ a sh:NodeShape ;
                    sh:property [ sh:datatype xsd:float ;
                            sh:minCount 1 ;
                            sh:minInclusive "0"^^xsd:float ;
                            sh:path rdf:value ],
                        [ sh:hasValue <%> ;
                            sh:minCount 1 ;
                            sh:path core:unit ] ] ;
            sh:path tto:PercentageExtension ],
        [ sh:minCount 1 ;
            sh:node [ a sh:NodeShape ;
                    sh:property [ sh:hasValue <mm> ;
                            sh:minCount 1 ;
                            sh:path core:unit ],
                        [ sh:datatype xsd:float ;
                            sh:minCount 1 ;
                            sh:minInclusive "0"^^xsd:float ;
                            sh:path rdf:value ] ] ;
            sh:path tto:OriginalWidth ],
        [ sh:datatype xsd:string ;
            sh:minCount 1 ;
            sh:path core:MaterialDesignation ],
        [ sh:minCount 1 ;
            sh:node [ a sh:NodeShape ;
                    sh:property [ sh:datatype xsd:float ;
                            sh:minCount 1 ;
                            sh:minInclusive "0"^^xsd:float ;
                            sh:path rdf:value ],
                        [ sh:hasValue <MPa> ;
                            sh:minCount 1 ;
                            sh:path core:unit ] ] ;
            sh:path tto:YieldStrength ],
        [ sh:minCount 1 ;
            sh:node [ a sh:NodeShape ;
                    sh:property [ sh:hasValue <mm> ;
                            sh:minCount 1 ;
                            sh:path core:unit ],
                        [ sh:datatype xsd:float ;
                            sh:minCount 1 ;
                            sh:minInclusive "0"^^xsd:float ;
                            sh:path rdf:value ] ] ;
            sh:path tto:OriginalThickness ],
        [ sh:minCount 1 ;
            sh:node [ a sh:NodeShape ;
                    sh:property [ sh:hasValue <MPa> ;
                            sh:minCount 1 ;
                            sh:path core:unit ],
                        [ sh:datatype xsd:float ;
                            sh:minCount 1 ;
                            sh:minInclusive "0"^^xsd:float ;
                            sh:path rdf:value ] ] ;
            sh:path tto:YoungsModulus ] ;
    sh:targetClass core:MechanicalTestingProcess .

